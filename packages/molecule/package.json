{
  "name": "@openchemistry/molecule",
  "version": "0.1.0",
  "description": "Stencil component to display chemical json data",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@openchemistry/vibrational-spectrum": "^0.0.0",
    "@openchemistry/molecule-moljs": "^0.3.0",
    "@openchemistry/cjson-utils": "^0.3.0",
    "@openchemistry/types": "^0.3.0",
    "@types/lodash-es": "^4.17.0",
    "lodash-es": "^4.17.10"
  },
  "devDependencies": {
    "@stencil/core": "^0.9.10",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenChemistry/oc-web-components"
  },
  "author": "Kitware, Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/OpenChemistry/oc-web-components"
  },
  "homepage": "https://github.com/OpenChemistry/oc-web-components",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
